#import <Foundation/Foundation.h>

@interface NSDate (CXYMCategory)


/**
 将15位时间转化为2018-12-12格式的时间

 @param dateString 格林尼治时间
 @return 2018-12-12格式的时间
 */
+ (NSString *)yyyyMMddWithDate:(NSString *)dateString;

/**
 将15位时间转化为2018-12-12  12:12格式的时间


 @param dateString 格林尼治时间
 @return 2018-12-12  12:12格式的时间
 */
+ (NSString *)yyyyMMddHHmmWithDate:(NSString *)dateString;
/**
 将15位时间转化为2018-12-12  12:12:12格式的时间
 
 
 @param dateString 格林尼治时间
 @return 2018-12-12  12:12:12格式的时间
 */
+ (NSString *)yyyyMMddHHmmssWithDate:(NSString *)dateString;



/**
 获取2个日期的间隔天数

 @param fromDate 开始日期
 @param toDate 结束日期
 @return 间隔的天数
 */
+ (NSUInteger )numberOfDaysFromDate:(NSDate *)fromDate toDate:(NSDate *)toDate;

@end
